public class Main {
    public static void main(String[] args) {
        // Entero
        int integer= 29;
        System.out.println("the range of Integer is -2147483648 to 2147483647");
        System.out.println("El int es "+ integer);

        //Caracater
        char caracater = 'k';
        Character caracaterObject;
        System.out.println("the range of Character is 0 to 65535");
        System.out.println("El char es " + caracater);

        //Boolean
        boolean booleanNumber = false;
        System.out.println("the range of Boolean is True or False ");
        System.out.println("El boolean es "+ booleanNumber);

        //Byte
        byte byteNumber = 15;
        System.out.println("the range of Byte is -128 to 127");
        System.out.println("El byte es "+ byteNumber);

        //Short
        short shortNumber = 44;
        System.out.println("the range of Short is -32768 to 32767");
        System.out.println("El short es " + shortNumber);

        //Decimal double
        double doubleNumber = 2.3435;
        System.out.println("the range of Double is  4.9e-324 to 1.8e308");
        System.out.println("El double es "+ doubleNumber);

        //Float / flotante
        float floatNumber = 22.674f;
        System.out.println("the range of Float is 1.4e-45 to 3.4e38 ");
        System.out.println("El float es "+ floatNumber);

        //Long
        long longNumber = 254367;
        System.out.println("the range of Long is -9223372036854775808 to 9223372036854775807");
        System.out.println("El long es "+ longNumber);
       
        //Cadena de caracteres
        String cadena = "Andres ";
        System.out.println("the range of String is 12+4+20=36 bytes");
        System.out.println("El String es "+ cadena);
       
        //Object
        Object objeto;
       
    }
}

